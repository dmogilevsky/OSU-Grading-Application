<p id="notice"><%= notice %></p>

<h1>Graderforms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @graderforms.each do |graderform| %>
      <tr>
        <td><%= link_to 'Show', graderform %></td>
        <td><%= link_to 'Edit', edit_graderform_path(graderform) %></td>
        <td><%= link_to 'Destroy', graderform, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Graderform', new_graderform_path %>
